{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
 	{
		"name": "client",
		"image": "neocycf/multi-client",
		"hostname": "client",
		"essential": false,		
		"memory": 128
	},
 	{
		"name": "server",
		"image": "neocycf/multi-server",
		"hostname": "api",
		"essential": false,		
		"memory": 128
	},
 	{
		"name": "worker",
		"image": "neocycf/multi-worker",
		"hostname": "worker",
		"essential": false,	
		"memory": 128
	},
 	{
		"name": "nginx",
		"image": "neocycf/multi-nginx",
		"essential": true,
		"portMappings": [
			{
				"hostPort": 80,
				"containerPort": 80	
			}
		],
		"links": ["client", "server"],
		"memory": 128
	}
  ]
}
